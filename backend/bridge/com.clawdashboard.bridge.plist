<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key><string>com.clawdashboard.bridge</string>
  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><dict><key>SuccessfulExit</key><false/></dict>
  <key>ThrottleInterval</key><integer>10</integer>
  <key>ProgramArguments</key><array>
    <string>/bin/bash</string>
    <string>/Users/mac/.openclaw/workspace/ClawDashboard/backend/bridge/run-bridge.sh</string>
  </array>
  <key>WorkingDirectory</key><string>/Users/mac/.openclaw/workspace/ClawDashboard/backend</string>
  <key>StandardOutPath</key><string>/Users/mac/.openclaw/workspace/ClawDashboard/backend/logs/bridge.log</string>
  <key>StandardErrorPath</key><string>/Users/mac/.openclaw/workspace/ClawDashboard/backend/logs/bridge-error.log</string>
</dict>
</plist>
